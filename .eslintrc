{
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "extends": ["eslint-config-leapfrog", "prettier"],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ]
    }
  }
{
  "name": "new-contact-manager",
  "version": "1.0.0",
  "description": "This is the backend of the contact manager application",
  "main": "src/app.js",
  "scripts": {
    "lint": "eslint src/*",
    "dev": "nodemon -r esm src/app",
    "seed": "npx knex seed:run --knexfile src/knexfile.js",
    "migrate": "npx knex migrate:latest --knexfile src/knexfile.js",
    "rollback": "npx knex migrate:rollback --knexfile src/knexfile.js",
    "make:seeder": "yarn knex seed:make --knexfile src/knexfile.js",
    "make:migration": "yarn knex migrate:make --knexfile src/knexfile.js"
  },
  "keywords": [
    "Contact Manager"
  ],
  "author": "Nikesh Sapkota",
  "license": "ISC",
  "dependencies": {
    "bookshelf": "^1.2.0",
    "bookshelf-virtuals-plugin": "^1.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^11.0.0",
    "esm": "^3.2.25",
    "express": "^4.17.2",
    "helmet": "^5.0.1",
    "http-status-codes": "^2.2.0",
    "knex": "^0.95.15",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1"
  }
}
